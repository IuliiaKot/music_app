<div class="container">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <ul>
        <% @bands.each do |album|%>
          <%= link_to album_path(album.id), class: 'my-link' do%>
            <ul class='search-result'><%= album.name%> (<%= album.year%>)</ul>
          <% end %>
        <% end %>
      </ul>
    </div>
  </div>
</div>
